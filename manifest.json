{
  "name": "CropIt",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": ["tabs", "<all_urls>"],
  "web_accessible_resources": [
        "fix.css"
    ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action":{
    "default_icon": "download.png"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "css": ["fix.css"]
  }
]
}
